@model IEnumerable<CaffeineBookatableProject.Models.Dinetable>

@{
    Layout = " ~/Views/Shared/_AdminLayoutPage.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Person_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Person_Number)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dine_Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dine_Time)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Outlet.outlet_name)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Person_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Person_Number)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dine_Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dine_Time)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Outlet.outlet_name)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Dine_Id }) |
                @Html.ActionLink("Details", "Details", new { id=item.Dine_Id }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.Dine_Id })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
